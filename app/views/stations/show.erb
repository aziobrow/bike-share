<a href='/stations'>Back to All Stations</a>
<h1>Viewing Station: <%=@station.name%></h1>
  <p>Dock Count: <%= @station.dock_count%></p>
  <p>City: <%= @station.city%></p>
  <p>Date Installed: <%= @station.installation_date.strftime("%m/%d/%Y")%></p>
  <form class="" action="/stations/<%= @station.id %>/edit" method="put">
    <input type="submit" name="_method" value="Edit">
  </form>
  <form action="/stations/<%= @station.id %>" method="post">
    <input type="submit" name="_method" value="Delete" />
  </form>
